<div class="card">
    <div class="card-header text-center">
        <h4>Devoured</h4>
    </div>
    <div class="card-body">
        {{#each devouredList}}
        <div>{{id}} {{burger_name}}</div>
        {{/each }}
    </div>
    {{#each undevouredList}}
    <div>
        <p>{{id}} {{burger_name}}
            <button class="btn btn-sm btn-outline-danger m-1 devour-btn float-right" data-burger="{{id}}">Devour<i
                    class="fas fa-utensils ml-2"></i>


            </button>
        </p>
    </div>
    {{/each }}
</div>